{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "rooms": {
      "type": "array",
      "items": {
        "oneOf": [
          {
            "type": "object",
            "required": ["!classroom", "id", "type", "name", "capacity"],
            "properties": {
              "!classroom": { "type": "null" },
              "id": { "type": "string", "pattern": "^cl[0-9]{2}$" },
              "type": { "const": "Classroom" },
              "name": { "type": "string" },
              "capacity": { "type": "integer", "minimum": 1 }
            }
          },
          {
            "type": "object",
            "required": ["!computerlab", "id", "type", "name", "capacity", "computer_count"],
            "properties": {
              "!computerlab": { "type": "null" },
              "id": { "type": "string", "pattern": "^pc[0-9]{2}$" },
              "type": { "const": "ComputerLab" },
              "name": { "type": "string" },
              "capacity": { "type": "integer", "minimum": 1 },
              "computer_count": { "type": "integer", "minimum": 1 }
            }
          },
          {
            "type": "object",
            "required": ["!laboratory", "id", "type", "name", "capacity", "equipment"],
            "properties": {
              "!laboratory": { "type": "null" },
              "id": { "type": "string", "pattern": "^lb[0-9]{2}$" },
              "type": { "const": "Laboratory" },
              "name": { "type": "string" },
              "capacity": { "type": "integer", "minimum": 1 },
              "equipment": { 
                "type": "array",
                "items": { "type": "string" }
              }
            }
          },
          {
            "type": "object",
            "required": ["!auditorium", "id", "type", "name", "capacity"],
            "properties": {
              "!auditorium": { "type": "null" },
              "id": { "type": "string", "pattern": "^au[0-9]{2}$" },
              "type": { "const": "Auditorium" },
              "name": { "type": "string" },
              "capacity": { "type": "integer", "minimum": 1 }
            }
          },
          {
            "type": "object",
            "required": ["!office", "id", "type", "name", "people"],
            "properties": {
              "!office": { "type": "null" },
              "id": { "type": "string", "pattern": "^of[0-9]{2}$" },
              "type": { "const": "Office" },
              "name": { "type": "string" },
              "people": {
                "type": "array",
                "items": { "type": "string" }
              }
            }
          },
          {
            "type": "object",
            "required": ["!studydepartment", "id", "type", "name", "faculty", "people", "contact", "hours"],
            "properties": {
              "!studydepartment": { "type": "null" },
              "id": { "type": "string", "pattern": "^sd[0-9]{2}$" },
              "type": { "const": "StudyDepartment" },
              "name": { "type": "string" },
              "faculty": { "type": "string" },
              "people": {
                "type": "array",
                "items": { "type": "string" }
              },
              "contact": { "type": "string" },
              "hours": {
                "type": "object",
                "properties": {
                  "open": { "type": "string", "pattern": "^([0-1][0-9]|2[0-3]):[0-5][0-9]$" },
                  "close": { "type": "string", "pattern": "^([0-1][0-9]|2[0-3]):[0-5][0-9]$" }
                },
                "required": ["open", "close"]
              }
            }
          },
          {
            "type": "object",
            "required": ["!deanery", "id", "type", "name", "people", "contact"],
            "properties": {
              "!deanery": { "type": "null" },
              "id": { "type": "string", "pattern": "^de[0-9]{2}$" },
              "type": { "const": "Deanery" },
              "name": { "type": "string" },
              "people": {
                "type": "array",
                "items": { "type": "string" }
              },
              "contact": { "type": "string" }
            }
          },
          {
            "type": "object",
            "required": ["!canteen", "id", "type", "name", "hours"],
            "properties": {
              "!canteen": { "type": "null" },
              "id": { "type": "string", "pattern": "^ca[0-9]{2}$" },
              "type": { "const": "Canteen" },
              "name": { "type": "string" },
              "hours": {
                "type": "object",
                "properties": {
                  "open": { "type": "string", "pattern": "^([0-1][0-9]|2[0-3]):[0-5][0-9]$" },
                  "close": { "type": "string", "pattern": "^([0-1][0-9]|2[0-3]):[0-5][0-9]$" }
                },
                "required": ["open", "close"]
              }
            }
          },
          {
            "type": "object",
            "required": ["!clubhouse", "id", "type", "name", "hours"],
            "properties": {
              "!clubhouse": { "type": "null" },
              "id": { "type": "string", "pattern": "^ch[0-9]{2}$" },
              "type": { "const": "Clubhouse" },
              "name": { "type": "string" },
              "hours": {
                "type": "object",
                "properties": {
                  "open": { "type": "string", "pattern": "^([0-1][0-9]|2[0-3]):[0-5][0-9]$" },
                  "close": { "type": "string", "pattern": "^([0-1][0-9]|2[0-3]):[0-5][0-9]$" }
                },
                "required": ["open", "close"]
              }
            }
          },
          {
            "type": "object",
            "required": ["!library", "id", "type", "name", "hours"],
            "properties": {
              "!library": { "type": "null" },
              "id": { "type": "string", "pattern": "^li[0-9]{2}$" },
              "type": { "const": "Library" },
              "name": { "type": "string" },
              "hours": {
                "type": "object",
                "properties": {
                  "open": { "type": "string", "pattern": "^([0-1][0-9]|2[0-3]):[0-5][0-9]$" },
                  "close": { "type": "string", "pattern": "^([0-1][0-9]|2[0-3]):[0-5][0-9]$" }
                },
                "required": ["open", "close"]
              }
            }
          },
          {
            "type": "object",
            "required": ["!wc", "id", "type", "gender"],
            "properties": {
              "!wc": { "type": "null" },
              "id": { "type": "string", "pattern": "^wc[0-9]{2}$" },
              "type": { "const": "WC" },
              "gender": { "enum": ["male", "female"] }
            }
          },
          {
            "type": "object",
            "required": ["!empty", "id", "type"],
            "properties": {
              "!empty": { "type": "null" },
              "id": { "type": "string", "pattern": "^em[0-9]{2}$" },
              "type": { "const": "Empty" }
            }
          }
        ]
      }
    }
  },
  "required": ["rooms"]
}
